{
    "name": "tysonandre/php-parser-to-php-ast",
    "description": "A tolerant-php-parser to php-ast converter written in php",
    "require": {
        "php": ">=7.1",
        "Microsoft/tolerant-php-parser": "0.0.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "suggest": {
        "ext-ast": "~0.1.5"
    },
    "autoload": {
        "psr-4": {"TolerantASTConverter\\": "src/TolerantASTConverter"}
    },
    "autoload-dev": {
        "psr-4": {"TolerantASTConverter\\Tests\\": "tests/TolerantASTConverter"}
    }
}
